pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build a code'
            }
        }
        stage('Test') {
            steps {
                echo 'Test software'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy software'
            }
        }
    }
}
